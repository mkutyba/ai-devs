{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.SemanticKernel": "Trace",
      "Agent": "Debug"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "RobotLogin": {
    "PageUrl": "",
    "Username": "",
    "Password": ""
  },
  "RobotVerifier": {
    "PageUrl": ""
  },
  "Hq": {
    "BaseUrl": "",
    "ApiKey": ""
  },
  "Ai": {
    "OpenAI": {
      "ApiKey": ""
    },
    "Ollama": {
      "ApiEndpoint": "http://localhost:11434"
    }
  }
}
